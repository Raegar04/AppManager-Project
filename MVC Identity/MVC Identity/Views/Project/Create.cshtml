@using MVC_Identity.ViewModels;
@model InfoProjectViewModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<form method="post" asp-action="Create">
    <div class="form-group">
        <label asp-for="Name">Title</label>
        <input asp-for="Name" class="form-control" placeholder="Enter title">
    </div><br>
    <div class="form-group">
        <label asp-for="Description">Description</label>
        <input asp-for="Description" class="form-control" placeholder="Description">
    </div><br>
    <div class="form-group">
        <label asp-for="Developer">Developer</label>
        <input asp-for="Developer" class="form-control" placeholder="Developer Name">
    </div><br>
    <div class="form-group">
        <label asp-for="Tester">Tester</label>
        <input asp-for="Tester" class="form-control" placeholder="Tester Name">
    </div><br>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>